<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
		<!-- 별칭 지정 -->
		<typeAliases>
			<typeAlias type="com.tis.retulix.domain.TrailerVO" alias="Trailer" />
			<typeAlias type="com.tis.retulix.domain.Trailer_ViewVO" alias="TrailerView" />
			<typeAlias type="com.tis.retulix.domain.ReviewVO" alias="Review" />
			<typeAlias type="com.tis.retulix.domain.Review_ViewVO" alias="ReviewView" />
			<typeAlias type="com.tis.retulix.domain.MemberVO" alias="Member" />
			<typeAlias type="com.tis.retulix.domain.NoticeVO" alias="Notice" />
			<typeAlias type="com.tis.retulix.domain.SubsVO" alias="Subs" />
			<typeAlias type="com.tis.retulix.domain.HistoryVO" alias="History" />
			<typeAlias type="com.tis.retulix.domain.Good_TrailerVO" alias="Good_Trailer" />
			<typeAlias type="com.tis.retulix.domain.Good_ReviewVO" alias="Good_Review" />
			<typeAlias type="com.tis.retulix.domain.Zzim_TrailerVO" alias="Zzim_Trailer" />
			<typeAlias type="com.tis.retulix.domain.Zzim_ReviewVO" alias="Zzim_Review" />
			<typeAlias type="com.tis.retulix.domain.Stat_ViewVO" alias="Stat_View" />
			<typeAlias type="com.tis.common.model.PagingVO" alias="Paging" />
		</typeAliases>
		
		<!-- 맵퍼 등록 -->
		<mappers>
			<mapper resource="spring/mapper/ChannelMapper.xml"/>
			<mapper resource="spring/mapper/mainMapper.xml"/>
			<mapper resource="spring/mapper/MemberMapper.xml"/>
			<mapper resource="spring/mapper/NoticeMapper.xml"/>
			<mapper resource="spring/mapper/searchMapper.xml"/>
			<mapper resource="spring/mapper/TrailerView_Mapper.xml" />
			<mapper resource="spring/mapper/TrailerMapper.xml"/>
			<mapper resource="spring/mapper/UserMapper.xml" />
			<mapper resource="spring/mapper/userRegMapper.xml"/>
		</mappers>
</configuration>